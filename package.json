{
  "name": "proxur",
  "version": "0.1.1",
  "type": "module",
  "main": "dist/index.js",
  "source": "src/index.js",
  "description": "Zero config, zero dependency, safe and lightweight (414 bytes gzipped!) proxy supporting both dot syntax and regular syntax.",
  "keywords": [
    "proxy",
    "safe",
    "dot path",
    "lodash path"
  ],
  "repository": "https://github.com/flowsn4ke/proxur",
  "author": {
    "name": "Theo Louvel",
    "url": "https://theolouvel.dev"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "microbundle-crl --format modern,cjs && rm dist/*.map",
    "dev": "microbundle-crl watch --no-compress --format modern,cjs",
    "test": "jest",
    "test:watch": "jest -i --watchAll"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "jest": "^27.4.5",
    "microbundle-crl": "^0.13.11"
  }
}